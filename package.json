{
	"version": "0.0.0",
	"name": "scrollyteller",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
		"lint": "prettier --plugin prettier-plugin-svelte --check . && eslint .",
		"format": "npx prettier --write --plugin prettier-plugin-svelte . --log-level warn"
	},
	"devDependencies": {
		"@sveltejs/adapter-static": "2.0.3",
		"@sveltejs/kit": "1.24.1",
		"eslint": "8.48.0",
		"eslint-config-prettier": "9.0.0",
		"eslint-plugin-svelte": "2.33.0",
		"prettier": "3.0.3",
		"svelte": "4.2.0",
		"svelte-preprocess": "5.0.4",
		"svelte2tsx": "0.6.21",
		"typescript": "5.2.2",
		"vite": "4.4.9"
	},
	"type": "module",
	"dependencies": {
		"archieml": "0.5.0"
	}
}
